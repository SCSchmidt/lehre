library(archdata)
data("BACups")
knitr::opts_chunk$set(echo = TRUE)
#CSV-Datei
BACups <- read.csv2("/home/sophie/R/lehre__upstream/analysis/data/BACups_tempered.csv",
sep = ";", # Separator zwischen den Spalten ist Semikolon
dec = ",") # Dezimaltrenner Komma
ggplot(data = BACups)+
geom_bar(aes(x = Phase, fill = Temper))+ # fill gibt den Balken unterschiedliche Farben, je nach den Angaben in der Spalte Temper
labs(y = "Häufigkeit",
title = "Vorkommen der zwei Phasen")+
theme_bw()
library(ggplot2) # 1. "Laden" des Pakets
ggplot(data = BACups)+
geom_bar(aes(x = Phase, fill = Temper))+ # fill gibt den Balken unterschiedliche Farben, je nach den Angaben in der Spalte Temper
labs(y = "Häufigkeit",
title = "Vorkommen der zwei Phasen")+
theme_bw()
ggplot(data = BACups)+
geom_point(aes(x = H, y = SD, shape = Phase, color = Temper)) +
labs(x =" Höhe des Gefäßes",
y ="Schulterdurchmesser",
title = "Höhe des Gefäßes im Verhältnis zum Schulterdurchmesser")+
theme_bw()
ggplot(data = BACups)+
geom_point(aes(x = H, y = SD, shape = TempCoarse, color = Temper)) +
labs(x =" Höhe des Gefäßes",
y ="Schulterdurchmesser",
title = "Höhe des Gefäßes im Verhältnis zum Schulterdurchmesser")+
theme_bw()
ggplot(data = BACups)+
geom_point(aes(x = H, y = SD, shape = Temper, color = TempCoarse)) +
labs(x =" Höhe des Gefäßes",
y ="Schulterdurchmesser",
title = "Höhe des Gefäßes im Verhältnis zum Schulterdurchmesser")+
theme_bw()
ggplot(data = BACups)+
